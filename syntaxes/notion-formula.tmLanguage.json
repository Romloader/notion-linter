{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Notion Formula",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.notion-formula",
					"match": "\\b(if)\\b"
				}
			]
		},
		"functions": {
			"patterns": [
				{
					"name": "entity.name.function.notion-formula",
					"match": "\\b(tests)\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.notion-formula",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.notion-formula",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "text.notion-formula"
}